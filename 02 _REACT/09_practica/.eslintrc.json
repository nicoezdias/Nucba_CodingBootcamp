{
   "extends": [
       "eslint:recommended",
       "plugin:import/errors",
       "plugin:react/recommended",
       "plugin:jsx-a11y/recommended",
       "prettier",
       "prettier/react"
   ],
   "plugins": ["react","import","jsx-a11y"],
   "rules": {
       "react/prop-types":0, // SI realmente necesitas validar tipos de datos (string, number, boolean, etc) Usa Typescript
       "no-console":1 //Warning
   },
   "parserOptions": {
       "ecmaVersion": 2021,
       "sourceType": "module",
       "ecmaFeatures": {
           "jsx": true
       }
   },
   "env": {
       "es6": true,
       "browser": true,
       "node": true
   },
   "settings": {
       "react":{
           "version": "detect"
       }
   } 
}